import java.util.*;

public class A1091246_0224_2 {
  public static void main(String[] argv) {
    int temperature1,a;
    
    System.out.print("請輸入任意攝氏溫度的值:");
    Scanner sc = new Scanner(System.in);
    temperature1 = sc.nextInt();
    
    a = temperature1 * 9 / 5 + 32;
    System.out.println("華氏溫度為:"+a);
  }
}
